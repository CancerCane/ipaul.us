{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "print-color-adjust",
          "user-select",
          "-webkit-overflow-scrolling",
          "print-color-adjust: exact"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "css-prefix-order": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "apple-touch-icons": "off"
  }
}